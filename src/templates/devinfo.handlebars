{{#ne_and dev_milestone 'live' 'deprecated'}}
    <div class="ia-single--info">
        <h3 class="ia-single--header">ID 
        {{#if permissions.admin}}
            {{#ne_and dev_milestone 'live' 'deprecated'}}
                <a href="#" class="devpage-edit {{#if staged.meta_id}}hide{{/if}}" id="dev-edit-meta_id">Edit</a>
                <a href="#" class="devpage-commit sep--after {{#unless staged.meta_id}}hide{{/unless}}" id="dev-commit-meta_id">Commit</a>
                <a href="#" class="devpage-cancel {{#unless staged.meta_id}}hide{{/unless}}" id="dev-cancel-meta_id">Cancel</a>
            {{/ne_and}}
        {{/if}}
        </h3>
        
        <span class="readonly--info {{#if staged.meta_id}}hide{{/if}}" id="meta_id--readonly">
            {{id}}
        </span>

        {{#if permissions.admin}}
            <input type="text" class=" {{#unless staged.meta_id}}hide{{/unless}} frm__input meta_id js-autocommit hidden-toshow" id="meta_id-input" value="{{#if staged.meta_id}}{{staged.meta_id}}{{else}}{{id}}{{/if}}" />
        {{/if}}
    </div>

    {{#if pr}}
        <div class="ia-single--info">
            <h3 class="ia-single--header">PR #</h3>
            <a href="https://github.com/duckduckgo/zeroclickinfo-{{repo}}/issues/{{pr.id}}" class="one-line" id="pr">
                {{pr.id}}
            </a>
        </div>
    {{/if}}

    <div class="ia-single--info">
        <h3 class="ia-single--header">Dev Machine 
        {{#if permissions.admin}}
            {{#ne_and dev_milestone 'live' 'deprecated'}}
                <a href="#" class="devpage-edit {{#if staged.test_machine}}hide{{/if}}" id="dev-edit-test_machine">Edit</a>
                <a href="#" class="devpage-commit sep--after {{#unless staged.test_machine}}hide{{/unless}}" id="dev-commit-test_machine">Commit</a>
                <a href="#" class="devpage-cancel {{#unless staged.test_machine}}hide{{/unless}}" id="dev-cancel-test_machine">Cancel</a>
            {{/ne_and}}
        {{/if}}
        </h3>
        
        {{#if permissions.admin}}
            <span class="frm__select hidden-toshow {{#unless staged.test_machine}}hide{{/unless}}">
            <select class="test_machine js-autocommit" tabindex="-1" id="test_machine-select">
                {{#if staged.test_machine}}
                    {{#with staged}}
                        {{#if test_machine}}<option value="0">{{test_machine}}</option>{{/if}}
                        <option value=""> --- </option>
                        {{#not_eq test_machine 'ddh1'}}<option value="1">ddh1</option>{{/not_eq}}
                        {{#not_eq test_machine 'ddh2'}}<option value="2">ddh2</option>{{/not_eq}}
                        {{#not_eq test_machine 'ddh3'}}<option value="3">ddh3</option>{{/not_eq}}
                        {{#not_eq test_machine 'ddh4'}}<option value="4">ddh4</option>{{/not_eq}}
                        {{#not_eq test_machine 'ddh5'}}<option value="5">ddh5</option>{{/not_eq}}
                        {{#not_eq test_machine 'ddh6'}}<option value="6">ddh6</option>{{/not_eq}}
                        {{#not_eq test_machine 'ddh7'}}<option value="7">ddh7</option>{{/not_eq}}
                        {{#not_eq test_machine 'ddh8'}}<option value="8">ddh8</option>{{/not_eq}}
                        {{#not_eq test_machine 'ddh9'}}<option value="9">ddh9</option>{{/not_eq}}
                        {{#not_eq test_machine 'ddh10'}}<option value="10">ddh10</option>{{/not_eq}}
                    {{/with}}
                {{else}}
                    {{#if test_machine}}<option value="0">{{test_machine}}</option>{{/if}}
                    <option value=""> --- </option>
                    {{#not_eq test_machine 'ddh1'}}<option value="1">ddh1</option>{{/not_eq}}
                    {{#not_eq test_machine 'ddh2'}}<option value="2">ddh2</option>{{/not_eq}}
                    {{#not_eq test_machine 'ddh3'}}<option value="3">ddh3</option>{{/not_eq}}
                    {{#not_eq test_machine 'ddh4'}}<option value="4">ddh4</option>{{/not_eq}}
                    {{#not_eq test_machine 'ddh5'}}<option value="5">ddh5</option>{{/not_eq}}
                    {{#not_eq test_machine 'ddh6'}}<option value="6">ddh6</option>{{/not_eq}}
                    {{#not_eq test_machine 'ddh7'}}<option value="7">ddh7</option>{{/not_eq}}
                    {{#not_eq test_machine 'ddh8'}}<option value="8">ddh8</option>{{/not_eq}}
                    {{#not_eq test_machine 'ddh9'}}<option value="9">ddh9</option>{{/not_eq}}
                    {{#not_eq test_machine 'ddh10'}}<option value="10">ddh10</option>{{/not_eq}}
                {{/if}}
            </select>
            </span>
        {{/if}}

        <span class="readonly--info {{#if staged.test_machine}}hide{{/if}}" id="test_machine--readonly">        
            {{#if test_machine}}
                {{test_machine}}
            {{else}}
                No dev machine
            {{/if}}
        </span>
    </div>
{{/ne_and}}

<div class="ia-attribution ia-single--info">
  {{#if developer}}
    <h3 class="ia-single--header">Contributors 
    {{#if permissions.can_edit}}
        {{#ne_and dev_milestone 'live' 'deprecated'}}
            <a href="#" class="devpage-edit-popup" id="dev-edit-contributors">Edit</a>
        {{/ne_and}}
    {{/if}}
    </h3>
    <ul>
      {{#each developer}}
        <li><a href="{{url}}" class="one-line">{{name}}</a></li>
      {{/each}}
    </ul>
  {{/if}}
</div>

<div class="ia-single--info">
  {{#if back_end}}
    <h3 class="ia-single--header">Back-End Files</h3>
    <ul>
      {{#each back_end}}
        <li>
        <a class="one-line" href="https://github.com/duckduckgo/zeroclickinfo-{{../repo}}/tree/master/{{this}}">{{final_path this}}</a>
        </li>
      {{/each}}
    </ul>
  {{/if}}
</div>

<div class="ia-single--info">
  {{#if front_end}}
    <h3 class="ia-single--header">Front-End Files</h3>
    <ul>
      {{#each front_end}}
        <li>
        <a class="one-line" href="https://github.com/duckduckgo/zeroclickinfo-{{../repo}}/tree/master/{{this}}">{{final_path this}}</a>
        </li>
      {{/each}}
    </ul>
  {{/if}}
</div>

<div class="hide edit-popup" id="contributors-popup">
    <h3 class="ia-single--header">Contributors</h3>
    {{#if permissions.admin}}
        <div>
          <input type="text" class="frm__input producer dev_milestone-container__body__input js-autocommit hidden-toshow team-input" id="producer-input" value="{{producer}}" />
          <div data-panel="contributors" class="dev_milestone-container__body__button js-autocommit hidden-toshow button assign-button btn" id="producer-button">
            Assign to me
          </div>
        </div>
    {{/if}}
    
    <ul>
        {{#if developer}}
            {{#each developer}}
                <li class="js-autocommit hidden-toshow developer">
                    <div data-panel="contributors" class="developer button delete left" name="developer" title="Remove developer">
                        <i class="ddgsi ddgsi-close-bold">X</i>
                    </div>
                    {{#eq_or type 'duck.co' 'github'}}
                    <span class="frm__select">
                        <select data-panel="contributors" class="available_types left" tabindex="-1" {{#eq type 'legacy'}}disabled="disabled"{{/eq}}>
                            {{#if type}}<option value="0">{{type}}</option>{{/if}}
                            {{#not_eq type 'duck.co'}}<option value="1">duck.co</option>{{/not_eq}}
                            {{#not_eq type 'github'}}<option value="2">github</option>{{/not_eq}}
                        </select>
                    </span>
                    {{/eq_or}}
                    <span class="developer_username left type-{{type}}">
                        <input type="text" data-panel="contributors" class="frm__input" {{#eq_or type 'duck.co' 'github'}}value="{{final_path url}}"{{/eq_or}} {{#eq type 'legacy'}}value="{{url}}" disabled="disabled"{{/eq}}/>
                    </span>
                </li>
                <span class="clearfix"></span>
            {{/each}}
        {{/if}}
        <li class="hide new_input js-autocommit hidden-toshow developer">
            <div data-panel="contributors" class="button delete left" name="developer" title="Remove developer">
                <i class="ddgsi ddgsi-close-bold">X</i>
            </div>
            <span class="frm__select">
              <select data-panel="contributors" class="available_types left" tabindex="-1">
                <option value="0">duck.co</option>
                <option value="1">github</option>
              </select>
            </span>
            <span class="developer_username left">
                <input data-panel="contributors" class="frm__input" type="text"/>
            </span>
            <span class="clearfix"></span>
        </li>
        <li id="add_developer">
            <div class="button add_input" id="add_developer_button" title="Add developer">
                <i class="ddgsi ddgsi-plus"></i>
            </div>
        </li>
    </ul>

    <div class="button left cancel-button-popup">Cancel</div>
    <div class="button left save-button-popup">Save</div>
</div>
