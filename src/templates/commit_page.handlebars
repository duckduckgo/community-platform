<h2>{{original.name}} - Staged Edits</h2>
<p>Click on the version you want to commit for each field</p>
<table class="staged_edits">
    <thead>
        <tr>
            <th class="empty_th"></th>
            <th>Live</th>
            <th>Edited</th>
         </tr>
    </thead>
    
    <tbody>
        {{#not_null name}}
            <tr class="updates_list">
                <th>Name</th>

                <td name="name" id="name_original">
                    {{original.name}}
                </td>
                <td name="name">{{name}}</td>
            </tr>
        {{/not_null}}
        
        {{#not_null id}}
            <tr class="updates_list">
                <th>ID</th>

                <td name="id" id="id_original">
                    {{original.id}}
                </td>
                <td name="id">{{id}}</td>
            </tr>
        {{/not_null}}

        {{#not_null status}}
            <tr class="updates_list">
                <th>Status</th>

                <td name="status" id="status_original">
                    {{original.status}}
                </td>
