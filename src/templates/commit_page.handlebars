                        <li data-url="{{url}}" data-type="{{type}}">
                                {{name}}
                        </li>
                    {{/each}}
                </td>
                <td name="developer">
                    {{#each developer}}
                        <li data-url="{{url}}" data-type="{{type}}">
                                {{name}}
                        </li>
                    {{/each}}
                </td>
            </tr>
        {{/not_null}}

        {{#not_null perl_module}}
